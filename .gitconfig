[user]
	name = Erik Westrup
	email = erik.westrup@gmail.com
	signingkey = AD610036
	editor = vim
[github]
	user = erikw
[core]
	excludesfile = ~/.gitignore_global
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
	page = true
[core]
[help]
	;browser = firefox -new-tab
	browser = lynx
[svn]
	rmdir = true
[alias]
	br = branch
	ci = commit
	cl = clone
	co = checkout
	di = diff
	diffc = diff --cached
	dic = diff --cached
	h = help
	cp = cherry-pick
	ft = fetch
	pu = push
	l = log
	log1 = log --pretty=oneline
	logs = log --stat
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
	rb = rebase
	st = status
	stat = status
	svn-status = diff --name-status remotes/git-svn
	sub = submodule
[credential]
	helper = cache
[push]
	default = upstream
[merge]
	log = true
[diff "odf"]
	textconv = odt2txt
; Companies using proxy needs a proto rewrite (set env https?_proxy too).
;[url "https://"]
	;insteadOf = git://

